// *9.3 (Use the Date class)
import java.util.*;

public class Ex9_3 {
	public static void main(String[] args) {
		Date[] date = new Date[8];
		long k = 10000;
		
		for (int i = 0; i < date.length; i++, k *= 10) {
			date[i] = new Date(k);
			System.out.println(k + " => " + date[i].toString());
		}
	}
}
