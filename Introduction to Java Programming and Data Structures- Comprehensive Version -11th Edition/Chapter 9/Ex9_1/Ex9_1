// 9.1 (The Rectangle class)

public class Ex9_1 {
	public static void main(String[] args) {
		Rectangle[] rect = {new Rectangle(4, 40),
				new Rectangle(3.5, 35.9)};
		
		for (int i = 0; i < rect.length; i++) {
		System.out.println("The width, height, area, perimeter of rectangle"
				+ (i + 1) + " are "
				+ rect[i].width + ", " + rect[i].height + ", "
				+ rect[i].getArea() + ", " + rect[i].getPerimeter());
		}
		
	}
}
